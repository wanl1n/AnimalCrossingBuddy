.navi-buttons {
    width: 61px;
    height: 70px;
    margin-bottom: 13px;
    -unity-background-image-tint-color: rgb(186, 183, 164);
    margin-top: 13px;
    justify-content: space-between;
    overflow: visible;
    transition-property: scale;
    transition-duration: 50ms;
    transition-timing-function: ease-out;
}

.navi-buttons VisualElement {
    width: 160%;
    height: 140%;
    justify-content: space-between;
    border-top-left-radius: 50%;
    border-top-right-radius: 50%;
    border-bottom-right-radius: 50%;
    border-bottom-left-radius: 50%;
    border-left-color: rgba(0, 0, 0, 0);
    border-right-color: rgba(0, 0, 0, 0);
    border-top-color: rgba(0, 0, 0, 0);
    border-bottom-color: rgba(0, 0, 0, 0);
    background-color: rgba(114, 93, 66, 0);
    align-self: center;
    margin-left: -18px;
    transition-property: background-color;
    transition-duration: 0.1s;
    transition-timing-function: ease-in;
}

.navi-buttons:hover VisualElement {
    -unity-background-image-tint-color: rgb(255, 255, 255);
    background-color: rgba(114, 93, 66, 0.12);
}

.navi-buttons:active {
    width: 61px;
    height: 70px;
    margin-bottom: 13px;
    margin-top: 13px;
    justify-content: space-between;
    padding-top: 0;
    padding-right: 0;
    padding-bottom: 0;
    padding-left: 0;
    scale: 1.2 1.2;
}

.navi-buttons:checked {
    width: 61px;
    height: 70px;
    margin-bottom: 13px;
    -unity-background-image-tint-color: rgb(253, 147, 3);
    margin-top: 13px;
    justify-content: space-between;
    padding-top: 0;
    padding-right: 0;
    padding-bottom: 0;
    padding-left: 0;
}

