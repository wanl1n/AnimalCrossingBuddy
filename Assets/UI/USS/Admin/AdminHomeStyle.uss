#HemisphereToggle #unity-checkmark {
    width: 90px;
    height: 90px;
    border-top-left-radius: 50px;
    border-top-right-radius: 50px;
    border-bottom-right-radius: 50px;
    border-bottom-left-radius: 50px;
    transition-property: translate, background-color, background-image, rotate, -unity-background-image-tint-color;
    transition-duration: 0.25s, 0.1s, 0.25s, 0.25s, 0.1s;
    background-image: url("project://database/Assets/Sprites/UI/Icons/NorthernHemisphere.png?fileID=2800000&guid=586d15c3c72c26047adc5f7111320d84&type=3#NorthernHemisphere");
    -unity-background-image-tint-color: rgb(255, 255, 255);
    transition-timing-function: ease-in-out, ease, ease, ease-out-cubic, ease;
    -unity-background-scale-mode: scale-to-fit;
    overflow: visible;
    background-color: rgba(186, 183, 164, 0);
    border-top-width: 0;
    border-right-width: 0;
    border-bottom-width: 0;
    border-left-width: 0;
}

#HemisphereToggle > VisualElement:checked #unity-checkmark {
    translate: 100% 0;
    background-image: url("project://database/Assets/Sprites/UI/Icons/SouthernHemisphere.png?fileID=2800000&guid=b7efda16ad96cfe43a7da2b00bff7cc7&type=3#SouthernHemisphere");
