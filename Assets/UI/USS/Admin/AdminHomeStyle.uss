}

#HemisphereToggle > VisualElement {
    background-color: rgb(248, 244, 232);
    overflow: visible;
    border-top-left-radius: 50px;
    border-top-right-radius: 50px;
    border-bottom-right-radius: 50px;
    border-bottom-left-radius: 50px;
    transition-property: background-color;
    transition-duration: 0.1s;
    padding-left: 10px;
}

#HemisphereToggle > VisualElement:hover #unity-checkmark {
    -unity-background-image-tint-color: rgb(198, 195, 186);
}

#HemisphereToggle:active #unity-checkmark {
